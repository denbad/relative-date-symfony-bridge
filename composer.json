{
    "name": "denbad/relative-date-symfony-bridge",
    "description": "Bridge for using denbad/relative-date in Symfony projects",
    "type": "symfony-bundle",
    "keywords": ["relative-date", "symfony", "bridge"],
    "homepage": "https://github.com/denbad/relative-date-symfony-bridge",
    "license": "MIT",
    "authors": [
        {
          "name": "Denis Badulin"
        }
    ],
    "support": {
        "issues": "https://github.com/denbad/relative-date-symfony-bridge/issues",
        "source": "https://github.com/denbad/relative-date-symfony-bridge"
    },
    "require": {
        "php": "^7.1",
        "denbad/relative-date": "@dev",
        "symfony/config": "~3.4",
        "symfony/dependency-injection": "~3.4",
        "symfony/http-kernel": "~3.4",
        "symfony/yaml": "~3.4",
        "symfony/translation": "~3.4"
    },
    "require-dev": {
        "symfony/var-dumper": "~3.4",
        "phpunit/phpunit": "^8.1",
        "phpstan/phpstan": "^0.11.5"
    },
    "repositories": [
        {
            "type":"git",
            "url": "git@github.com:denbad/relative-date.git"
        }
    ],
    "autoload": {
        "psr-4": {"Denbad\\RelativeDateSymfonyBridge\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"Denbad\\RelativeDateSymfonyBridge\\Tests\\": "tests"}
    }
}
